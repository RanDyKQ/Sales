luid: true, // 当true时，Video.js player将拥有流体大小。换句话说，它将按比例缩放以适应其容器\r\n                sources: [\r\n                    {\r\n                        src: 'https://cdn.cnbj1.fds.api.mi-img.com/mi-mall/94e8483d022ba06a4a131efd0fa104e3.mp4', // 路径\r\n                        type: 'video/mp4'\r\n                    },\r\n                    {\r\n                        src: '//path/to/video.webm',\r\n                        type: 'video/webm'\r\n                    }\r\n                ],\r\n                poster: 'https://cdn.cnbj1.fds.api.mi-img.com/mi-mall/2e839b5e97a0f84586a2098453bf0663.jpg', // 你的封面地址\r\n                // width: document.documentElement.clientWidth,\r\n                notSupportedMessage: '此视频暂无法播放，请稍后再试', // 允许覆盖Video.js无法播放媒体源时显示的默认信息\r\n                controlBar: {\r\n                    timeDivider: true,\r\n                    durationDisplay: true,\r\n                    remainingTimeDisplay: false,\r\n                    fullscreenToggle: true // 全屏按钮\r\n                }\r\n            }\r\n        }\r\n    },\r\n    /* 监视 */\r\n    watch: {\r\n        '$route.query.itemId' () {\r\n            this.itemId = this.$route.query.itemId\r\n        }\r\n    },\r\n    computed: {\r\n        itemInfo () {\r\n            let itemInfo = this.itemData.filter((item) => {\r\n                return Number(item.sku_id) === Number(this.itemId)\r\n            })[0]\r\n            return itemInfo\r\n        }\r\n    },\r\n    methods: {\r\n        /* 实现图片更换的方法 */\r\n        tableImg (index) {\r\n            console.log(index)\r\n            this.imgIndex = index\r\n        },\r\n        /* 添加购物车的方法 */\r\n        addCarpanelData () {\r\n            let itemData = { info: this.itemInfo, count: this.count }\r\n            this.$store.commit('addCarpanelData', itemData)\r\n        },\r\n        /* 数量减少 */\r\n        subcount () {\r\n            this.count--\r\n            if (this.count < 1) {\r\n                this.count = 1\r\n            }\r\n        },\r\n        /* 数量增加 */\r\n        addcount () {\r\n            this.count++\r\n            if (this.count > this.itemInfo.limit_num) {\r\n                this.count = this.itemInfo.limit_num\r\n            }\r\n        },\r\n        /* 点击弹出按钮 */\r\n        popBox () {\r\n            var popBox = document.getElementById('popBox')\r\n            popBox.style.display = 'block'\r\n        },\r\n\r\n        /* 点击关闭按钮 */\r\n        closeBox () {\r\n            var popBox = document.getElementById('popBox')\r\n            popBox.style.display = 'none'\r\n        }\r\n    },\r\n    components: {\r\n        /* 商品达到最大购买量时的弹窗 */\r\n        prompt,\r\n        headerNav,\r\n        footerNav,\r\n        videoPlayer\r\n    }\r\n}\r\n</script>\r\n<style>\r\n/*弹出层*/\r\n#popBox {\r\n    display: none;\r\n    background-color: #000;\r\n    z-index: 11;\r\n    width: 900px;\r\n    height: 450px;\r\n    position:fixed;\r\n    top:0;\r\n    right:0;\r\n    left:0;\r\n    bottom:0;\r\n    margin:auto;\r\n    position: relative;\r\n}\r\n#popBox .close{\r\n    text-align: right;\r\n    margin-right: 5px;\r\n    background-color: #d6d3d3;\r\n}\r\n#popBox .content{\r\n    width: 700px;\r\n    line-height: 30px;\r\n}\r\n#popBox .content .h1{\r\n    position: absolute;\r\n    color: #fff;\r\n    left: 6%;\r\n    top: 20%;\r\n}\r\n#popBox .content .h2{\r\n    position: absolute;\r\n    color: #fff;\r\n    left: 36%;\r\n    top: 20%;\r\n}\r\n#popBox .content .h3{\r\n    position: absolute;\r\n    color: #fff;\r\n    left: 6%;\r\n    top: 35%;\r\n}\r\n#popBox .content .h4{\r\n    position: absolute;\r\n    color: #fff;\r\n    left: 36%;\r\n    top: 35%;\r\n}\r\n#popBox .content .h5{\r\n    position: absolute;\r\n    color: #fff;\r\n    left: 6%;\r\n    top: 50%;\r\n}\r\n#popBox .content .h6{\r\n    position: absolute;\r\n    color: #fff;\r\n    left: 36%;\r\n    top: 50%;\r\n}\r\n#popBox .content .h7{\r\n    position: absolute;\r\n    color: #fff;\r\n    left: 6%;\r\n    top: 65%;\r\n}\r\n#popBox .co