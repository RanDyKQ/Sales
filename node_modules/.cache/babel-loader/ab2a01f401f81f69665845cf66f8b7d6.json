AnB,CAA2B,UAAAC,KAAK,EAAI;AAClC,YAAIA,KAAK,CAACO,MAAN,KAAiBF,IAAI,CAACE,MAA1B,EAAkC;AAChCP,UAAAA,KAAK,CAACF,KAAN;;AACA,cAAIE,KAAK,CAACF,KAAN,GAAcE,KAAK,CAACQ,SAAxB,EAAmC;AACjCR,YAAAA,KAAK,CAACF,KAAN;AACAN,YAAAA,KAAK,CAACE,MAAN,GAAe,IAAf;AACAY,YAAAA,IAAI,GAAG,KAAP;AACA;AACD;;AACDA,UAAAA,IAAI,GAAG,KAAP;AACAd,UAAAA,KAAK,CAACG,OAAN,GAAgB,IAAhB;AACD;AACF,OAZD;;AAaA,UAAIW,IAAJ,EAAU;AACRd,QAAAA,KAAK,CAACG,OAAN,GAAgB,IAAhB;AACA,YAAIc,SAAS,GAAGJ,IAAhB;AACAjB,QAAAA,GAAG,CAACsB,GAAJ,CAAQD,SAAR,EAAmB,OAAnB,EAA4B,CAA5B;AACAjB,QAAAA,KAAK,CAACC,YAAN,CAAmBkB,IAAnB,CAAwBF,SAAxB;AACD;AACF,KAvBQ;;AAwBT;AACAG,IAAAA,eAzBS,2BAyBQpB,KAzBR,EAyBeqB,EAzBf,EAyBmB;AAC1BrB,MAAAA,KAAK,CAACC,YAAN,CAAmBM,OAAnB,CAA2B,UAACC,KAAD,EAAQc,KAAR,EAAkB;AAC3C,YAAId,KAAK,CAACO,MAAN,KAAiBM,EAArB,EAAyB;AACvBrB,UAAAA,KAAK,CAACC,YAAN,CAAmBsB,MAAnB,CAA0BD,KAA1B,EAAiC,CAAjC;AACA,iBAAOtB,KAAP;AACD;AACF,OALD;AAMD,KAhCQ;;AAiCT;AACAwB,IAAAA,WAlCS,uBAkCIxB,KAlCJ,EAkCW;AAClBA,MAAAA,KAAK,CAACE,MAAN,GAAe,KAAf;AACD,KApCQ;;AAqCT;AACAuB,IAAAA,OAtCS,mBAsCAzB,KAtCA,EAsCO;AACdA,MAAAA,KAAK,CAACG,OAAN,GAAgB,IAAhB;AACD,KAxCQ;;AAyCT;AACAuB,IAAAA,OA1CS,mBA0CA1B,KA1CA,EA0CO;AACd2B,MAAAA,UAAU,CAAC,YAAM;AACf3B,QAAAA,KAAK,CAACG,OAAN,GAAgB,KAAhB;AACD,OAFS,EAEP,GAFO,CAAV;AAGD;AA9CQ,GA5BiB;AA4E5ByB,EAAAA,OAAO,EAAE,EA5EmB;AA8E5BC,EAAAA,OAAO,EAAE;AA9EmB,CAAf,CAAf","sourcesContent":["/* 引入vue和vuex */\nimport Vue from 'vue'\nimport Vuex from 'vuex'\n\nVue.use(Vuex)\n\nexport default new Vuex.Store({\n  state: {\n    /* 存放数据 */\n    carPanelData: [],\n    /* 是否为最大值 */\n    maxOff: false,\n    /* 控制隐藏显示 */\n    carShow: false\n  },\n  /* 购物车中的计算 */\n  getters: {\n    /* 数量 */\n    totalCount (state) {\n      let count = 0\n      state.carPanelData.forEach(goods => {\n        count += goods.count\n      })\n      return count\n    },\n    /* 价格计算 */\n    totalPrice (state) {\n      let price = 0\n      state.carPanelData.forEach(goods => {\n        price += goods.count * goods.price\n      })\n      return price\n    }\n  },\n  mutations: {\n    addCarpanelData (state, data) {\n      /* let是判断购物车是否添加过 */\n      let bOff = true// eslint-disable-line no-unused-vars\n      state.carPanelData.forEach(goods => {\n        if (goods.sku_id === data.sku_id) {\n          goods.count++\n          if (goods.count > goods.limit_num) {\n            goods.count--\n            state.maxOff = true\n            bOff = false\n            return\n          }\n          bOff = false\n          state.carShow = true\n        }\n      })\n      if (bOff) {\n        state.carShow = true\n        let goodsData = data\n        Vue.set(goodsData, 'count', 1)\n        state.carPanelData.push(goodsData)\n      }\n    },\n    /* 删除购物车商品 */\n    delCarpanelData (state, id) {\n      state.carPanelData.forEach((goods, index) => {\n        if (goods.sku_id === id) {\n          state.carPanelData.splice(index, 1)\n          return state\n        }\n      })\n    },\n    /* 关闭弹窗 */\n    closePrompt (state) {\n      state.maxOff = false\n    },\n    /* 显示购物车 */\n    showCar (state) {\n      state.carShow = true\n    },\n    /* 隐藏购物车 */\n    hideCar (state) {\n      setTimeout(() => {\n        state.carShow = false\n      }, 500)\n    }\n  },\n  actions: {\n  },\n  modules: {\n  }\n})\n"]}]}